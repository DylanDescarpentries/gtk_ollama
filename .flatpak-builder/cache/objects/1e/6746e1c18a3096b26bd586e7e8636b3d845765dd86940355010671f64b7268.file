[Application]
name=org.descarpentries.gtk_ollama
runtime=org.gnome.Platform/x86_64/47
sdk=org.gnome.Sdk/x86_64/47
command=gtk_ollama

[Context]
shared=network;ipc;
sockets=x11;wayland;fallback-x11;
devices=dri;

[Extension org.descarpentries.gtk_ollama.Debug]
directory=lib/debug
autodelete=true
no-autodownload=true

[Build]
built-extensions=org.descarpentries.gtk_ollama.Debug;